<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="100.0" prefWidth="800.0" style="-fx-background-color: rgba(0, 0, 0, 0);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.controller.NavPaneController">
   <children>
      <ImageView fx:id="logo" fitHeight="92.0" fitWidth="172.0" onMouseClicked="#loadMainPage" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
         <image>
            <Image url="@LOGO.png" />
         </image>
      </ImageView>
      <Label fx:id="navTOP" layoutX="220.0" layoutY="40.0" onMouseClicked="#loadTopPosts" style="-fx-cursor: hand;" text="TOP" textFill="#aeaeae">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="navHOT" layoutX="341.0" layoutY="40.0" onMouseClicked="#loadHotPosts" style="-fx-cursor: hand;" text="HOT" textFill="#aeaeae">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="navFOLLOWED" layoutX="475.0" layoutY="40.0" onMouseClicked="#loadFollowedPosts" style="-fx-cursor: hand;" text="FOLLOWED" textFill="#aeaeae">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
   </children>
</Pane>
